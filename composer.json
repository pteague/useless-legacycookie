{
	"name": "useless/legacycookie",
	"description": "Helper for massive legacy applications that are using cookies and need to be switched over to sessions",
	"type": "library",
	"keywords": [
		"Cookie"
		,"Session"
	],
	"homepage": "https://github.com/pteague/useless-legacycookie",
	"license": "MIT",
	"authors": [
		{
			"name": "Patrick Teague",
			"role": "developer"
		}
	],
	"support": {
		"source": "https://github.com/pteague/useless-legacycookie"
	},
	"require": {
		"php": ">=5.3.10"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
	},
	"autoload": {
		"psr-4": {
			"Useless\\Legacy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Useless\\Legacy\\": "test/src/"
		}
	}
	,"include-path": [
		"./"
	]
	,"minimum-stability": "dev"
	,"prefer-stable": true
}
